    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "mongoose": "^5.9.6",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2"
  }
}
